            <div class="row">
                <div class="col-lg-8 col-lg-offset-2 col-md-10 col-md-offset-1 blog-content">
                  <div class="post-preview">
                    <% @articles.each do |article| %>
                      <tr>
                        <td><h2 class="post-title"><%= article.title %></h2></td>
                        <td><h3 class="post-subtitle"><%= article.text %></h3></td>
                        <td><button class="btn btn-default"><%= link_to 'Visualizar', article_path(article) %></button></td>
                        <td><button class="btn btn-warning"><%= link_to 'Editar', edit_article_path(article) %></button></td>
                        <td><button class="btn btn-danger"><%= link_to 'Deletar', article_path(article),
                                method: :delete,
                                data: { confirm: 'Quer mesmo deletar o Post?' } %></button></td>
                      </tr>
                    <% end %>
                  </div>

                  <p><button class="btn btn-primary"><%= link_to 'Novo Post', new_article_path %></button></p>

